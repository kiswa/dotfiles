conky.config = {
alignment = 'middle_right',
background = true,
border_width = 2,
cpu_avg_samples = 2,
default_color = 'eeeeee',
default_outline_color = 'white',
default_shade_color = '333333',
draw_borders = false,
draw_graph_borders = true,
draw_outline = false,
draw_shades = true,
use_xft = true,
font = 'Nota Sans:size=11',
xftalpha = 0.8,
gap_x = 15,
gap_y = 15,
no_buffers = true,
out_to_console = false,
out_to_stderr = false,
extra_newline = false,
own_window = true,
own_window_type = 'normal',
own_window_transparent = false,
own_window_hints = 'undecorated,below,sticky,skip_taskbar,skip_pager',
own_window_argb_visual = true,
own_window_argb_value = 125,
stippled_borders = 0,
update_interval = 1.0,
uppercase = false,
use_spacer = 'right',
show_graph_scale = false,
show_graph_range = false,
double_buffer = true,
minimum_width = 420,
maximum_width = 420
}

conky.text = [[
${alignr}${exec whoami} @ $nodename
${alignr}$sysname $kernel $machine$color
$hr
${color bbbbbb}Uptime:$color $uptime_short
${color bbbbbb}RAM Usage:$color $memperc%${goto 260}${membar 10}
${color bbbbbb}CPU Usage:$color $cpu%${goto 260}${cpubar 10}
${alignc}${cpugraph cpu0 50,90 29E6C6 E62949 -t}  ${cpugraph cpu1 50,90 29E6C6 E62949 -t}\
  ${cpugraph cpu2 50,90 29E6C6 E62949 -t}  ${cpugraph cpu3 50,90 29E6C6 E62949 -t}
${alignc}${cpugraph cpu4 50,90 29E6C6 E62949 -t}  ${cpugraph cpu5 50,90 29E6C6 E62949 -t}\
  ${cpugraph cpu6 50,90 29E6C6 E62949 -t}  ${cpugraph cpu7 50,90 29E6C6 E62949 -t}
${alignc}${cpugraph cpu8 50,90 29E6C6 E62949 -t}  ${cpugraph cpu9 50,90 29E6C6 E62949 -t}\
  ${cpugraph cpu10 50,90 29E6C6 E62949 -t}  ${cpugraph cpu11 50,90 29E6C6 E62949 -t}
${alignc}${cpugraph cpu12 50,90 29E6C6 E62949 -t}  ${cpugraph cpu13 50,90 29E6C6 E62949 -t}\
  ${cpugraph cpu14 50,90 29E6C6 E62949 -t}  ${cpugraph cpu15 50,90 29E6C6 E62949 -t}
${alignc}${cpugraph cpu16 50,90 29E6C6 E62949 -t}  ${cpugraph cpu17 50,90 29E6C6 E62949 -t}\
  ${cpugraph cpu18 50,90 29E6C6 E62949 -t}  ${cpugraph cpu19 50,90 29E6C6 E62949 -t}
${alignc}${color bbbbbb}CPU Frequency:$color ${freq_g}GHz
${alignc}${color bbbbbb}Processes:$color $processes${color bbbbbb}Running:$color $running_processes
${alignc}${color bbbbbb}Battery:$color ${battery_short} ${battery_time}
$stippled_hr
${color bbbbbb}File System Usage:$color
 /${alignr}${fs_used /}/${fs_size /}
 ${fs_bar 10,330 /}${alignr}${fs_used_perc /}%
 /home${alignr}${fs_used /home}/${fs_size /home}
 ${fs_bar 10,330 /home}${alignr}${fs_used_perc /home}%
$stippled_hr
${color bbbbbb}Networking:
 Up: $color${upspeed wlp0s20f3}${goto 220}${color bbbbbb}Down: $color${downspeed wlp0s20f3}
 ${downspeedgraph wlp0s20f3 70,200 29E6C6 E62949 -t}${goto 220}\
${upspeedgraph wlp0s20f3 70,200 29E6C6 E62949 -t}
$stippled_hr
${alignc}${color bbbbbb}Desktop$color ${desktop} ${color bbbbbb}of$color ${desktop_number}
#Update weather every 5 minutes
${alignc}${color bbbbbb}${texeci 200 ~/bin/weather.py}
]]
